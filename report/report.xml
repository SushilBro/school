<?xml version="1.0" encoding="UTF-8"?>
<odoo>
   
    <record id="report_student_pdf" model="ir.actions.report">
        <field name="name">Student Card</field>
        <field name="model">school.student</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">samata_school.report_student_card_id</field>
        <field name="report_file">samata_school.report_student_card_id</field>
        <field name="binding_model_id" ref="model_school_student" />
        <field name="binding_type">report</field>
    </record>

     <record id="report_student_details_xls" model="ir.actions.report">
        <field name="name">Student Details Excel</field>
        <field name="model">school.student</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">samata_school.report_student_details_xl</field>
        <field name="report_file">samata_school.report_student_details_xl</field>
        <field name="binding_model_id" ref="model_school_student" />
        <field name="binding_type">report</field>
    </record>

